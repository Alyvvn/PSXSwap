{
  "functions": {
    "app/api/pfp-assets/upload/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/uploads/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/game/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/game/(.*)",
      "destination": "/game/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/uploads/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
